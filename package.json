{
    "name": "camelize-minimist",
    "version": "1.0.0",
    "description": "recursively transform key strings to camel-case, with a _ twist",
    "main": "index.js",
    "devDependencies": {
        "tape": "~2.3.2"
    },
    "scripts": {
        "test": "tape test/*.js"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/forivall/camelize-minimist.git"
    },
    "homepage": "https://github.com/forivall/camelize-minimist",
    "keywords": [
        "camel-case",
        "json",
        "transform",
        "minimist"
    ],
    "testling" : {
        "files" : "test/*.js",
        "browsers" : {
            "iexplore" : [ "6.0", "7.0", "8.0", "9.0" ],
            "chrome" : [ "20.0" ],
            "firefox" : [ "10.0", "15.0" ],
            "safari" : [ "5.1" ],
            "opera" : [ "12.0" ]
        }
    },
    "author": "Jordan J Klassen <forivall@gmail.com> (http://forivall.com)",
